---
// Per un componente che richiede React Query (es. UsersComponent)
import Layout from '../layouts/Layout.astro'
import WrapperComponent from '../components/tsx/base/WrapperComponent'

//const { componentType = 'users' } = Astro.props
const { componentType = 'posts' } = Astro.props
const useQueryString = true
---

<Layout title="Dynamic Component Page">
  <WrapperComponent
    client:load
    componentType={componentType}
    useQueryString={useQueryString}
    additionalProps={{ someValue: 123, someValue1: 345 }}
  />
</Layout>
